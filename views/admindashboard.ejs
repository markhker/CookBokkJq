<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="es_MX"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>
    <meta name="description" content="">
    <meta name="author" content="Mark Hkr">
    <meta property="fb:admins" content="100002353510548">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="https://use.fontawesome.com/f4cee09130.js"></script>
    <script src="dist/vendor/jquery-2.2.4.min.js"></script>
    <script src="dist/vendor/jquery-cropit.min.js"></script>
    <link rel="stylesheet" href="dist/css/main.css">
  </head>
  <body>
    <% if(loaded){ %>
      <script>
        $('body').addClass('loaded');
      </script>
    <% } else { %>
      <script>
        $('body').removeClass('loaded');
      </script>
     <% } %>
    <!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    
    <!-- PreLoader -->
    <div id="loader-wrapper">
        <div id="loader"></div>
        <div class="loader-section section-left"></div>
        <div class="loader-section section-right"></div>
    </div>
    <!-- /PreLoader -->

    <div class="containerAdminDashboard col1">
      
      <div class="adminMenu col1">
        <a href="#!" class="adminMenuItem">Appetizers</a>
        <a href="#!" class="adminMenuItem">Soups, Salads & Sides</a>
        <a href="#!" class="adminMenuItem">Main Dishes</a>
        <a href="#!" class="adminMenuItem">Desserts</a>
      </div>

      <h1 class="adminTitle col1">
        All Recipes
      </h1>
      
      <div class="adminMainDash col1">
        
        <% for(var i=0; i < keys; i++) { %>
         <div class="col724">
          <div class="adminItem col1">
            <div class="adminItemBack col1" style="background-image: url(<%= data[i].dishImage %>);"></div>
            
            <% if(data[i].recipe.approval == 0) { %>
              <div class="adminBanner no">
                <p class="adminBannerTitle">
                  Not yet approved
                <img src="https://s3-us-west-2.amazonaws.com/elasticbeanstalk-us-west-2-513199747384/images/admin/reject.png" alt="reject" class="adminBannerImg">
              </div>
            <% } else if(data[i].recipe.approval == 1) { %>
              <div class="adminBanner yes">
                <p class="adminBannerTitle">
                  Approved
                </p>
                <img src="https://s3-us-west-2.amazonaws.com/elasticbeanstalk-us-west-2-513199747384/images/admin/check.png" alt="check" class="adminBannerImg">
              </div>
            <% } %>
            
            <div class="adminItemContent col1">
              <h1 class="adminItemTitle">
                <%= data[i].recipe.recipeTitle %>
              </h1>
              <p class="adminItemName col1">
                <%= data[i].firstName + ' ' + data[i].lastName %>
              </p>
              <p class="adminItemLocation col1">
                <%= data[i].city %>
              </p>
              <p class="adminItemPosition col1">
                <%= data[i].position %>
              </p>
              <div class="adminItemUserImage" style="background-image: url(<%= data[i].userImage %>)"></div>
            </div>
          </div>
          
          <div class="adminReviewButtons col1">
            <a href="#" class="btn adminReview" data-key="<%= 'cookdata-'+ data[i].employeeid +'.json' %>">Review</a>
            <% if(data[i].recipe.approval == 0) { %>
              <a href="#" class="btn adminPublish" data-key="<%= 'cookdata-'+ data[i].employeeid +'.json' %>">Publish</a>
            <% } else if(data[i].recipe.approval == 1) { %>
              <a href="#" class="btn adminPublish adminDeactivated" data-key="<%= 'cookdata-'+ data[i].employeeid +'.json' %>">Publish</a>
            <% } %>
          </div>
         </div>
        <% } %>
        
      </div>


      <footer class="row mainFooter">
        <div class="col824 colm624 coll324">
          <a href="index.html"><img src="https://s3-us-west-2.amazonaws.com/elasticbeanstalk-us-west-2-513199747384/images/logo-taste.png" alt="Taste logo" class="imgResponsive"></a>
        </div>
      </footer>
    </div>
    <!-- /container -->

    <!--script src="dist/vendor/dropzone.js"></script-->
    <script src="src/js/uploadForm.js"></script>
    <script src="src/js/main.js"></script>
  </body>
</html>
